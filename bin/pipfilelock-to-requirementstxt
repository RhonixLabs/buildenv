#!/usr/bin/jq -rf
# https://github.com/pypa/pipenv/issues/3493
# Not using hashes because https://stackoverflow.com/a/50695493/214720
.default | to_entries[] |
	(select(.value.git)	|
		"git+" + .value.git + "@" + .value.ref
	) //
	(select(.value.version)	|
		.key + .value.version
	) //
	error("Unsupported dependency in Pipfile.lock: " +  (. | tostring))
